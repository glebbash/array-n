{
  "name": "array-n",
  "version": "1.0.0",
  "description": "Create multidimensional arrays in javascript",
  "main": "dist/array.js",
  "scripts": {
    "test": "tsc && nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glebbash/array-n.git"
  },
  "keywords": [
    "array",
    "multi",
    "dimension"
  ],
  "author": "glebbash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glebbash/array-n/issues"
  },
  "homepage": "https://github.com/glebbash/array-n#readme",
  "prettier": {
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "recursive": true,
    "spec": "tests/**.test.ts"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
